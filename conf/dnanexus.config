params {
  refSource = "datahub"
}

process {
  withName:trackStart {
    executor = 'dnanexus'
  }
  withName:getBag {
    executor = 'dnanexus'
  }
  withName:getData {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:parseMetadata {
    executor = 'dnanexus'
  }
  withName:trimData {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:getRefInfer {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:downsampleData {
    executor = 'dnanexus'
  }
  withName:alignSampleData {
    machineType = 'mem3_ssd1_v2_x16'
  }
  withName:inferMetadata {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:checkMetadata {
    executor = 'dnanexus'
  }
  withName:getRef {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:alignData {
    queue = '256GB,256GBv1'
  }
  withName:dedupData {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:countData {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:makeBigWig {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:fastqc {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:dataQC {
    machineType = 'mem1_ssd1_v2_x16'
  }
  withName:aggrQC {
    executor = 'dnanexus'
  }
  withName:uploadInputBag {
    executor = 'dnanexus'
  }
  withName:uploadExecutionRun {
    executor = 'dnanexus'
  }
  withName:uploadQC {
    executor = 'dnanexus'
  }
  withName:uploadProcessedFile {
    executor = 'dnanexus'
  }
  withName:uploadOutputBag {
    executor = 'dnanexus'
  }
  withName:finalizeExecutionRun {
    executor = 'dnanexus'
  }
  withName:failPreExecutionRun {
    executor = 'dnanexus'
  }
  withName:failExecutionRun {
    executor = 'dnanexus'
  }
  withName:uploadQC_fail {
    executor = 'dnanexus'
  }
}

docker {
  enabled = true
}
